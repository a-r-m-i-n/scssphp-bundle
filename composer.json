{
    "name": "armin/scssphp-bundle",
    "version": "2.0.0-dev",
    "type": "symfony-bundle",
    "description": "Symfony bundle to parse scss sources at runtime.",
    "license": "MIT",
    "authors": [
        {
            "name": "Armin Vieweg",
            "email": "armin@v.ieweg.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-json": "*",
        "symfony/framework-bundle": "^4.0 | ^5.0 | ^6.0",
        "symfony/twig-bridge": "^4.0 | ^5.0 | ^6.0",
        "symfony/asset": "^4.0 | ^5.0 | ^6.0",
        "scssphp/scssphp": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Armin\\ScssphpBundle\\": "src/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        }
    }
}
